<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" >
  <class name="ACS.Models.Po.Business.AccessDetail,ACS" table="TAccessItems"  >

    <id name="AccessDetailID" column ="AccessID" >
      <generator class ="identity"/>
    </id>
    
    <property name="AccessID">
      <column name="AccessNID"/>
    </property>
    
    <property name="AccessName">
      <column name="AccessName" length="50"/>
    </property>
    
    <property name="ValueID">
      <column name="DoorID"/>
    </property>

    <property name="Type">
      <column name="AccessType" length="20"/>
    </property>

    <property name="CreateUserID">
      <column name="CreateUserID"/>
    </property>

    <property name="CreateDate">
      <column name="CreateDate"/>
    </property>

  </class>
</hibernate-mapping>



