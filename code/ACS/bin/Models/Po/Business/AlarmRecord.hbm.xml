<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" >
  <class name="ACS.Models.Po.Business.AlarmRecord,ACS" table="t_alarm_record"  >

    <id name="AlarmID" column ="ALARM_ID" >
      <generator class ="identity"/>
    </id>

    <property name="AlarmTime" type="DateTime">
      <column name="ALARM_TIME"/>
    </property>

    <property name="ControlID">
      <column name="CONTROL_ID"/>
    </property>

    <property name="DoorID">
      <column name="DOOR_ID"/>
    </property>

    <property name="EventTypeID">
      <column name="EVENT_TYPE_ID"/>
    </property>
    
  </class>
</hibernate-mapping>