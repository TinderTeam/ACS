<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" >
  <class name="ACS.Models.Po.Business.Holiday,ACS" table="t_holiday"  >

    <id name="HolidayID" column ="HOLIDAY_ID" >
      <generator class ="identity"/>
    </id>

    <property name="HolidayName">
      <column name="HOLIDAY_NAME" length="50"/>
    </property>
    
    <property name="StartTime">
      <column name="START_TIME"/>
    </property>

    <property name="EndTime">
      <column name="END_TIME"/>
    </property>

    <property name="HolidayNote">
      <column name="HOLIDAY_NOTE" length="100"/>
    </property>
    
  </class>
</hibernate-mapping>