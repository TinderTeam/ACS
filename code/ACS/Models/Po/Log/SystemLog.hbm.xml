<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" >
  <class name="ACS.Models.Po.Log.SystemLog,ACS" table="t_system_log"  >

    <id name="LogID" column ="LOG_ID" >
      <generator class ="increment"/>
    </id>

    <property name="LogDateTime" type="DateTime">
      <column name="LOG_DATE_TIME"/>
    </property>

    <property name="UserCode" type="int">
      <column name="USER_CODE" />
    </property>
    
    <property name="LogEvent" type="string">
      <column name="LOG_EVENT" length="255"/>
    </property>

    <property name="Msg" type="DateTime">
      <column name="MSG"/>
    </property>
    
    <property name="Result" type="string">
      <column name="RESULT"/>
    </property>

    <property name="Computer" type="string">
      <column name="COMPUTER"/>
    </property>

  </class>
</hibernate-mapping>