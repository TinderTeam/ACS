<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" >
  <class name="ACS.Models.Po.Business.AccessDetailView,ACS" table="t_access_detail_view"  >
    
    <id name="AccessDetailID" column ="ACCESS_DETAIL_ID" >
      <generator class ="increment"/>
    </id>
    
    <property name="AccessID" type="int">
      <column name="ACCESS_ID"/>
    </property>
    
    <property name="ValueID" type="int">
      <column name="VALUE_ID"/>
    </property>
    
    <property name="Type" type="string">
      <column name="TYPE" length="255"/>
    </property>

    <property name="CreateUserID" type="int">
      <column name="CREATE_USER_ID"/>
    </property>
    
    <property name="CreateDate">
      <column name="CREATE_DATE"/>
    </property>
    
    <property name="AccessName" type="string">
      <column name="ACCESS_NAME" length="255"/>
    </property>
    
    <property name="ControlID" type="int">
      <column name="CONTROL_ID"/>
    </property>
    
    <property name="ControlName" type="string">
      <column name="CONTROL_NAME" length="255"/>
    </property>

    <property name="DoorID" type="int">
      <column name="DOOR_ID"/>
    </property>
    
    <property name="DoorName" type="string">
      <column name="DOOR_NAME" length="255"/>
    </property>

    <property name="DoorTimeName" type="string">
      <column name="DOOR_TIME_NAME" length="255"/>
    </property>
    
    <property name="StartTime">
      <column name="START_TIME"/>
    </property>

    <property name="EndTime">
      <column name="END_TIME"/>
    </property>

  </class>
</hibernate-mapping>