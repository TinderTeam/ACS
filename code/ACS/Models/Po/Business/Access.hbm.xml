<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" >
  <class name="ACS.Models.Po.Business.Access,ACS" table="t_access"  >

    <id name="AccessID" column ="ACCESS_ID" >
      <generator class ="increment"/>
    </id>

    <property name="AccessName" type="string">
      <column name="ACCESS_NAME" length="255"/>
    </property>
    
    <property name="AccessNote" type="string">
      <column name="ACCESS_NOTE" length="255"/>
    </property>

    <property name="AcsString" type="string">
      <column name="ACS_STRING" length="255"/>
    </property>

    <property name="CreateUserID" type="int">
      <column name="CREATE_USER_ID"/>
    </property>
    
    <property name="CreateDate" type="DateTime">
      <column name="CREATE_DATE"/>
    </property>

  </class>
</hibernate-mapping>