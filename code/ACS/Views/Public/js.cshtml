
<script type="text/javascript">
    //用于员工性别显示
    function onSexRenderer(e) {
        if (e.value == 1) return "男";
        else return "女";
    }
    //
    function onYesRenderer(e) {
        if (e.value == 1) return "是";
        else return "否";
    }
    //用于显示控制器状态
    function onlineStatusRenderer(e) {
        if (e.value == true) return "在线";
        else return "离线";
    }
    //用于显示控制器类型
    function controlTypeRenderer(e) {
        if (e.value == 1) return "单门";
        else if (e.value == 2) return "2门";
        else return "4门";
    }
</script>