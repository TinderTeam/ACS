<?xml version="1.0" encoding="utf-8" ?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" >
  <class name="ACS.Models.Po.Business.EventRecord,ACS" table="TEvent"  >

    <id name="EventID" column ="EventID" >
      <generator class ="identity"/>
    </id>

    <property name="CardNo">
      <column name="CardNo" length="11"/>
    </property>
    
    <property name="EventTime">
      <column name="EventTime"/>
    </property>

    <property name="ControlID">
      <column name="ControlID"/>
    </property>

    <property name="DoorID">
      <column name="DoorID"/>
    </property>

    <property name="EmployeeID">
      <column name="EmployeeID"/>
    </property>

    <property name="EventTypeID">
      <column name="EventType"/>
    </property>

    <property name="Modify">
      <column name="Modify"/>
    </property>
    
  </class>
</hibernate-mapping>

